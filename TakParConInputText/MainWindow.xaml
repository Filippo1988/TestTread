<Window x:Class="TakParConInputText.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TakParConInputText"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnStartTask" Content="Start" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="238,180,0,0" Click="btnStartTask_Click"/>
        <Label x:Name="lblCount"  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="238,246,0,0"/>
        <TextBox x:Name="txtInitial" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="238,112,0,0"/>

    </Grid>
</Window>
